<Application
    x:Class="MyerSplash.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:common="using:MyerSplash.Common"
    xmlns:local="using:MyerSplash"
    xmlns:vm="using:MyerSplash.ViewModel">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="/ResourceDictionary/ColorDictionary.xaml" />
                <ResourceDictionary Source="/ResourceDictionary/StyleDictionary.xaml" />
                <ResourceDictionary Source="/ResourceDictionary/StringDictionary.xaml" />
                <ResourceDictionary Source="/ResourceDictionary/DimentionDictionary.xaml" />
                <ResourceDictionary Source="ms-appx:///MyerSplashCustomControl/Themes/CommonStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Light" Source="/ResourceDictionary/LightColorDictionary.xaml" />
                <ResourceDictionary x:Key="Dark" Source="/ResourceDictionary/DarkColorDictionary.xaml" />
            </ResourceDictionary.ThemeDictionaries>
            <vm:ViewModelLocator x:Key="Locator" />
            <common:AppSettings x:Key="AppSettings" />
            <x:String x:Key="CoachKey">TIPS_NETWORK</x:String>
            <x:String x:Key="GestureKey">TIPS_GESTURE</x:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>